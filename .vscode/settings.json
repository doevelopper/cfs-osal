{
    "files.exclude": {
      "out/[^D]*/": true,
      "out/Debug/[^g]*": false,
      "out/Debug/g[^e]*": false,
      "out_*/**": true,
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/.idea": true,
      "**/node_modules": true,
      "**/bazel-bin": true,
      "**/bazel-out": true,
      "**/bazel-testlogs": true,
      "**/target": true,
      "**/bazel-cfs-osal": true
    },

    "files.hotExit": "onExit",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/**": true,
      "**/dist/**": true,
      "**/public_gen/**": true
    },

    "C_Cpp.clang_format_path": "${workspaceRoot}/.clang-format",
    "C_Cpp.clang_format_sortIncludes": false,
    "C_Cpp.clang_format_formatOnSave": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.rightClickBehavior": "copyPaste",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.rendererType": "dom",

    "editor.tabSize": 2,
    "workbench.colorCustomizations" : {
      "terminal.foreground" : "#1abe3ee3",
      "terminal.background" : "#020f299d",
      "terminal.ansiBrightBlue":"#babd0fe5",
      "terminalCursor.foreground": "#ebebe1",
      "titleBar.activeBackground": "#f3fefe",
      "titleBar.activeForeground": "#0f0f16",
      "titleBar.inactiveBackground": "#0f0f16",
      "titleBar.inactiveForeground": "#f3fefe",
      "statusBar.background": "#055737",
      "editorLineNumber.foreground": "#9e9797ea",
      "editor.background": "#201e1ee0",
      "editor.lineHighlightBackground": "#050a0e",
      "editor.lineHighlightBorder": "#70460fe0",
      "editor.selectionBackground": "#0a3464",
      //"sideBar.background": "#ebebe4",
      //"sideBar.foreground": "#030303",
      //"sideBarTitle.foreground": "#0a0a0a",
      //"tab.activeForeground": "#ff0000",
    },
    "files.associations": {
      "functional": "cpp",
      "cctype": "cpp",
      "clocale": "cpp",
      "cmath": "cpp",
      "cstdarg": "cpp",
      "cstddef": "cpp",
      "cstdio": "cpp",
      "cstdlib": "cpp",
      "cstring": "cpp",
      "ctime": "cpp",
      "cwchar": "cpp",
      "cwctype": "cpp",
      "array": "cpp",
